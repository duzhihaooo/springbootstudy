<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
		PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springbootstudymybatis.dao.UserDao">
 
        <select id="selectUserRoleList" resultType="com.example.springbootstudymybatis.pojo.UserRole">
            select u.user_name,r.role_name
            from
            user u,role r where u.id = r.user_id;
        </select>
 
    
 </mapper>